{
  "name": "render",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "watch-development": "env-cmd -f .env-development npm run watch",
    "watch-goerli": "env-cmd -f .env-goerli npm run watch",
    "watch-mainnet": "env-cmd -f .env-mainnet npm run watch",
    "watch": "nodemon --watch 'src/**/*.ts' --ext ts,js --exec npm run start",
    "start": "env-cmd -f .env ts-node src/index.ts",
    "start-test": "env-cmd -f .env ts-node src/test.ts",
    "watch-test": "nodemon --watch 'src/**/*.ts' --ext ts,js --exec npm run start-test",
    "prod-goerli": "env-cmd -f .env-goerli npm run prod",
    "prod-mainnet": "env-cmd -f .env-mainnet npm run prod",
    "prod": "env-cmd -f .env.production ts-node src/index.ts",
    "migrate-development": "env-cmd -f .env-development npx knex migrate:latest",
    "migrate-rollback-development": "env-cmd -f .env-development npx knex migrate:rollback",
    "migrate-production": "env-cmd -f .env-mainnet npx knex migrate:latest"
  },
